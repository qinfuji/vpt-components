{
	"name": "vpt-components",
	"version": "1.0.0",
	"description": "A UI component library from VPT.",
	"main": "dist/index.js",
	"module": "lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/qinfuji/vpt.git"
	},
	"scripts": {
		"build": "yarn run build-js && yarn run build-modules",
		"build-bundle": "scripts/build-bundle.sh",
		"build-docs": "rm -rf ./dist/docs && NODE_ENV=documentation build-storybook -o ./dist/docs",
		"build-js": "NODE_ENV=production babel --extensions .js,.jsx --ignore spec.js,docs,examples,test-entry.js,coverage ./src --out-dir ./dist",
		"build-modules": "NODE_ENV=production BABEL_ENV=modules babel --extensions .js,.jsx --ignore spec.js,docs,examples,test-entry.js,coverage ./src --out-dir ./lib",
		"cover": "yarn run jest -- --coverage",
		"create-component": "scripts/create-component.sh",
		"dev": "yarn run storybook",
		"docs": "NODE_ENV=documentation start-storybook  -p 6006",
		"jest": "NODE_ENV=test TZ=UTC jest --runInBand",
		"path-import-modules-generate": "node ./scripts/path-import-modules-exec.js --generate",
		"path-import-modules-remove": "node ./scripts/path-import-modules-exec.js --remove",
		"prettier": "prettier --write '{src,tests,scripts}/**/*.{js,jsx}'",
		"preversion": "rm -rf dist && yarn run lint && yarn run build-js && yarn run build-bundle &&  yarn run path-import-modules-generate",
		"release": "np",
		"start": "yarn run docs",
		"storybook": "start-storybook -p 6006",
		"test": "yarn run lint && yarn run cover",
		"watch": "yarn run jest -- --watch",
		"ii": "yarn install --registry https://registry.npm.taobao.org"
	},
	"keywords": [
		"component",
		"ecosystem-react",
		"react",
		"react-component",
		"reactive",
		"stateful",
		"stateless"
	],
	"author": "qinfuji",
	"license": "Apache-2.0",
	"dependencies": {
		"classnames": "^2.2.3",
		"create-react-class": "^15.6.2",
		"d3-format": "^1.0.2",
		"d3-scale": "^1.0.0",
		"d3-shape": "^1.0.0",
		"d3-time": "^1.0.0",
		"d3-time-format": "^2.0.0",
		"element-resize-detector": "^1.1.6",
		"glamorous": "^4.13.0",
		"inline-style-prefixer": "^4.0.2",
		"lodash": "^4.11.0",
		"prop-types": "^15.6.0",
		"react-addons-shallow-compare": "^15.6.2",
		"react-day-picker": "^6.2.1",
		"react-motion": "^0.5.2",
		"react-peek": "^1.0.5",
		"react-transition-group": "^1.2.1",
		"reselect": "^2.5.1",
		"styled-components": "^3.2.6",
		"typescript": "^3.0.1"
	},
	"devDependencies": {
		"@sambego/storybook-state": "^1.0.7",
		"@storybook/addon-actions": "^3.4.6",
		"@storybook/addon-info": "^3.3.12",
		"@storybook/addon-knobs": "^3.4.6",
		"@storybook/addon-links": "^3.3.12",
		"@storybook/addon-notes": "^3.4.6",
		"@storybook/addon-options": "^3.3.14",
		"@storybook/react": "^3.3.12",
		"autoprefixer": "^8.2.0",
		"awesome-typescript-loader": "^5.2.0",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.4",
		"babel-minify-webpack-plugin": "^0.3.1",
		"babel-plugin-import": "^1.6.3",
		"babel-plugin-istanbul": "^4.1.3",
		"babel-plugin-lodash": "^3.2.11",
		"babel-plugin-module-resolver": "^3.0.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-require-ignore": "^0.1.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babylon": "^6.0.0",
		"css-loader": "^0.28.11",
		"enzyme": "^3.1.0",
		"enzyme-adapter-react-16": "^1.0.2",
		"enzyme-to-json": "^3.1.4",
		"eslint": "^4.15.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-import-resolver-babel-module": "^4.0.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "^7.5.1",
		"glob": "^7.1.1",
		"husky": "^0.13.3",
		"jest": "^21.2.1",
		"jscodeshift": "^0.5.0",
		"less": "^2.3.1",
		"less-loader": "^4.1.0",
		"lint-staged": "^3.4.0",
		"marksy": "^6.0.3",
		"mini-css-extract-plugin": "^0.4.0",
		"np": "^2.20.1",
		"postcss-loader": "^2.1.3",
		"prettier": "^1.1.0",
		"raw-loader": "^0.5.1",
		"react": "^16.3.0",
		"react-docgen-typescript-loader": "^2.2.0",
		"react-docgen-typescript-webpack-plugin": "^1.1.0",
		"react-dom": "^16.3.0",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"react-syntax-highlighter": "^7.0.0",
		"sinon": "^2.0.0-pre",
		"storybook-readme": "^3.0.6",
		"style-loader": "^0.20.3",
		"stylelint": "^7.10.1",
		"stylelint-config-standard": "^16.0.0",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "^4.5.0",
		"webpack-cli": "^2.0.14",
		"webpack-dev-server": "^3.1.1"
	}
}
